class Solution {
public:
    vector<int> searchRange(vector<int>& v, int t) {
        int n = v.size();
        vector<int> ans;
        int count = 0;
        for(int i=0;i<n;i++)
        {
            if(v[i] == t){  
                ans.push_back(i);
                count++;
            }
        }
        int v2 = ans.size();

        if(!count) return {-1,-1};
        return {ans[0],ans[v2-1]};
    }
};

//Store the index in vector and return the min and element of the vector
